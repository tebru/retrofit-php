{
    "name": "tebru/retrofit-php",
    "description": "Retrofit for PHP - A PHP REST client.",
    "require": {
        "php": ">= 5.4",
        "guzzlehttp/guzzle": "~5.0",
        "jms/serializer": "~0.11",
        "doctrine/annotations": "~1.0",
        "twig/twig": "~1.12",
        "symfony/filesystem": "~2.6",
        "symfony/console": "^2.0.5",
        "nikic/PHP-Parser": "~1.2",
        "tebru/assert": "0.1.*"
    },
    "require-dev": {
        "php": ">= 5.5",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.4.*",
        "satooshi/php-coveralls": "^0.6.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nate Brunette",
            "email": "n@tebru.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tebru\\Retrofit\\": "src/",
            "Tebru\\Retrofit\\Test\\": "tests/"
        }
    },
    "bin": ["bin/retrofit"],
    "config": {
        "bin-dir": "bin"
    }
}
