{
    "name": "tebru/retrofit-php",
    "description": "Retrofit for PHP - A PHP REST client.",
    "require": {
        "php": ">= 5.4",
        "jms/serializer": ">=0.11, <2.0",
        "symfony/console": "^2.0.5",
        "tebru/assert": "^0.2",
        "tebru/dynamo": "^0.3.0",
        "tebru/retrofit-http-clients": "^0.6.6",
        "symfony/event-dispatcher": "^2.3",
        "guzzlehttp/psr7": "^1.0",
        "monolog/monolog": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.4.*",
        "guzzlehttp/guzzle": "^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nate Brunette",
            "email": "n@tebru.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tebru\\Retrofit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tebru\\Retrofit\\Test\\": "tests/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to make requests"
    },
    "bin": ["bin/retrofit"]
}
